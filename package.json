{
  "name": "angular2-starter-basic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf aot dist && rm -rf app/*.js",
    "lite": "lite-server",
    "copy:jit": "cp support/main-jit.ts app/main.ts && cp support/index-jit.html index.html",
    "copy:aot": "cp support/main-aot.ts app/main.ts && cp support/index-aot.html index.html",
    "ngc": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js",
    "build:jit": "npm run copy:jit && tsc",
    "build:aot": "npm run copy:jit && npm run ngc && npm run copy:aot && npm run ngc && npm run rollup",
    "dev": "npm run clean && npm run build:jit && npm run lite",
    "prod": "npm run clean && npm run build:aot && npm run lite"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "concurrently": "^3.2.0",
    "lite-server": "^2.2.2",
    "typescript": "~2.2.0",
    "rollup": "0.41.4",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-uglify": "1.0.1"
  },
  "dependencies": {
    "@angular/common": "4.1.2",
    "@angular/compiler": "4.1.2",
    "@angular/core": "4.1.2",
    "@angular/forms": "4.1.2",
    "@angular/http": "4.1.2",
    "@angular/platform-browser": "4.1.2",
    "@angular/platform-browser-dynamic": "4.1.2",
    "@angular/router": "4.1.2",
    "@angular/upgrade": "4.1.2",
    "@angular/compiler-cli": "4.1.2",
    "@angular/platform-server": "4.1.2",
    "systemjs": "0.19.40",
    "core-js": "^2.4.1",
    "rxjs": "5.0.1",
    "zone.js": "^0.8.4",
    "reflect-metadata": "0.1.10"
  }
}
